<script>
$(document).ready(function()
{

  
  var $slider = $('#slider_out'); //div
  var $slidesContainer = $slider.find('.slides_in'); //ul
  var $slide = $slidesContainer.find('.slide_data'); // li

  var animationSpeed = 4000;
  var pause = 1000;
  var width = 1200;
  var currentSlide = 3;

  //images = ["1558027346070.jpg","3.jpg","1.jpg","1557078704640.jpg","1563639100367.jpg","1556899377959.jpg","1556968308766.jpg","1556889462194.jpg","1558169613861.jpg"];

  var interval; 
  function startSlider()
  {
  interval = setInterval(function()
          {

            $slidesContainer.animate({'margin-left':'-='+width},animationSpeed,function()
            {

              currentSlide = currentSlide + 3;
              //alert(currentSlide)
              if(currentSlide === $slide.length)
              {

                currentSlide=0;
                $slidesContainer.css('margin-left',0);

              } 
            });
          },pause);
  }

  function stopSlider()
  {
    clearInterval(interval);
  }

  $slide.on('mouseenter',stopSlider).on('mouseleave',startSlider);
startSlider();
})
</script>